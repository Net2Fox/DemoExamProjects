<Window x:Class="GIBDD_Gorlanov_619.Windows.CodeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GIBDD_Gorlanov_619"
        mc:Ignorable="d"
        Title="Двухфакторная аутентификация" Height="200" Width="200" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBlock Text="Введите код для входа:" TextAlignment="Center"/>
            <TextBox x:Name="CodeTextBox" Margin="10,20,10,0"/>
            <Button x:Name="ConfirmButton" Content="Подтвердить" Margin="10,20,10,0" Click="ConfirmButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
