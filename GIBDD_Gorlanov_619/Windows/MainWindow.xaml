<Window x:Class="GIBDD_Gorlanov_619.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GIBDD_Gorlanov_619"
        mc:Ignorable="d"
        Title="Госавтоинспекция" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="BackButton" HorizontalAlignment="Left" VerticalAlignment="Center" Width="50" Content="Назад" Margin="10,0,0,0" Click="BackButton_Click"/>
        <Button x:Name="ExitButton" HorizontalAlignment="Right" VerticalAlignment="Center"  Width="50" Content="Выйти" Margin="0,0,10,0" Click="ExitButton_Click"/>
        <Frame x:Name="MainFrame" Grid.Row="1" Source="/Pages/LoginPage.xaml" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
